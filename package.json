{
  "name": "world-trends-explorer",
  "version": "1.0.3",
  "description": "Real-time Google Trends explorer with interactive world map using Pytrends",
  "main": "frontend/index.html",
  "scripts": {
    "start": "./start.sh",
    "build": "./scripts/deploy.sh",
    "serve": "cd frontend && python -m http.server 8000",
    "backend": "cd backend && python app.py",
    "mock": "cd backend && python mock_server.py",
    "test": "jest tests/",
    "test:coverage": "jest tests/ --coverage",
    "test:watch": "jest tests/ --watch",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gon1zau6jyun4liu5/world-trends-explorer.git"
  },
  "keywords": [
    "google-trends",
    "data-visualization",
    "world-map",
    "pytrends",
    "d3js",
    "flask",
    "interactive-map"
  ],
  "author": "World Trends Explorer Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gon1zau6jyun4liu5/world-trends-explorer/issues"
  },
  "homepage": "https://github.com/gon1zau6jyun4liu5/world-trends-explorer#readme",
  "dependencies": {},
  "devDependencies": {
    "jest": "^29.0.0",
    "@testing-library/jest-dom": "^5.16.0"
  },
  "engines": {
    "node": ">=14.0.0",
    "python": ">=3.8.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": ["<rootDir>/tests/setup.js"],
    "collectCoverageFrom": [
      "frontend/js/**/*.js",
      "!frontend/js/**/*.test.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
